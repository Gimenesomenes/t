##Determinante de uma matriz 3x3


det=function(A){
	Dt=0	
	i=1
	for(j in 1:n){
	if(j==1){
		D=(A[i+1,j+1]*A[i+2,j+2])-(A[i+1,j+2]*A[i+2,j+1])
	if(j==2){
		D=(A[i+1,j-1]*A[i+2,j+1])-(A[i+2,j-1]*A[i+1,j+1])
	if(j==3){
		D=(A[i+1,j-2]*A[i+2,j-1])-(A[i+2,j-2]*A[i+1,j-1])
	}
	Dt=Dt+((-1)^(i+j)M[i,j]*D)
	}
return(Dt)
}

			
